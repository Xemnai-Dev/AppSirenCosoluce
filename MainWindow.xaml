<Window x:Class="AppSiren.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AppSiren"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <TextBox x:Name="SiretInput" HorizontalAlignment="Left" Margin="41,117,0,0" Text="" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="SirenInput" HorizontalAlignment="Left" Margin="41,257,0,0" Text="" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <Label Content="Siren" HorizontalAlignment="Left" Margin="63,231,0,0" VerticalAlignment="Top"/>
        <Label Content="Siret" HorizontalAlignment="Left" Margin="63,86,0,0" VerticalAlignment="Top"/>
        <Button Name="SiretButton" Content="Verifier" HorizontalAlignment="Left" Margin="271,117,0,0" VerticalAlignment="Top" Width="59" Click="VerifySiret"/>
        <Button Name="SirenButton" Content="Verifier" HorizontalAlignment="Left" Margin="271,257,0,0" VerticalAlignment="Top" Width="59" Click="VerifySiren"/>
    </Grid>
</Window>
